import java.util.*;
class Palidrome{
    public static void main(String[]args){
        Scanner scan = new Scanner(System.in);
        System.out.println("\t\t\t Conversion of Number to String ");
         for(int i=1;i<6;i++){
        System.out.println(i+".)Enter the Integer Value :");
        int number   = scan.nextInt();
        if (number>0 &&  number<27){
        char character =  (char)(number+64);
        System.out.println("In the number of"+" "+ number+" " +"is"+" " +character );
       }else{
        System.out.println("It is not right number ");
       }
         }
    }
}
