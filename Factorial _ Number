import java.util.*;
public class GivenPrimeNumberSeries {
    public static void main(String[] args) {
       System.out.println("Factorial number");
       int number = 4;
       int factorial =1;
       for(int i=1;i<=number;i++){
           factorial = factorial * i;
       }
      System.out.println(factorial);
    }
}
